{
  "extends": "@devvit/public-api/devvit.tsconfig.json",
  "compilerOptions": {
    "allowJs": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "noEmitOnError": true,
    "noImplicitAny": false,
    "sourceMap": true,
    "target": "ES2016",
    "typeRoots": [
      "node_modules/@types/",
      "typings/index.d.ts"
    ]
  },
  "lib": [
    "ES2017",
    "DOM"
  ],
  "noEmit": true,
  "types": [
    "vitest/globals"
  ],
  "exclude": [
    "node_modules",
    "**/*-aot.ts"
  ]
}